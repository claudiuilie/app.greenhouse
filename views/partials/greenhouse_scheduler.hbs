<div class="mt-3">
    <h5>
        <i class="fal fa-calendar-alt"></i> Scheduler
        <small>
            <i class="bi bi-chevron-down"></i>
        </small>
    </h5>
</div>
<div class="container">
    <div class="row row-cols-3 p-1 justify-content-center">
        <div class="col p-1">
            <div class="card shadow mb-3 rounded-lg">
                <div class="card-body p-0">
                    {{#each scheduler}}
                        {{#if this.active}}
                            {{this.lamp_start}}
                            {{this.lamp_stop}}
                            {{this.max_temp}}
                            {{this.min_temp}}
                            {{this.max_humidity}}
                            {{this.min_humidity}}
                        {{/if}}
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</div>

